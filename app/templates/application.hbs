<div class="l-main-window-container">
		{{#each flashMessages.queue as |flash|}}
			{{#flash-message flash=flash as |component flash|}}
				{{#alert-body status=component.flashType iconSize="sm" centerContent=true}}
					{{flash.message}}
				{{/alert-body}}
	  	{{/flash-message}}
		{{/each}}

			{{outlet}}
</div>

{{outlet 'modal'}}
